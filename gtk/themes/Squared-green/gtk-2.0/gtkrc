	bg[ACTIVE]		= "#617953"
	bg[INSENSITIVE]		= "#222222"
	bg[PRELIGHT]		= "#224212"
	bg[SELECTED]		= "#617953"

	fg[SELECTED]		= "#ffffff"
}

